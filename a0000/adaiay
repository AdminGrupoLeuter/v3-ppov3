NEWBASE ADAIA_DB TYPE(ORACLE) 
        LOCATION("adaiasrv.latam") ACCESS(WRITE=YES,SHARE=YES) 
        CONNECT(USER='ppov3',PASSWORD='ppov3');

USING ADAIA_DB;

   NEWFILE ACCCOL TYPE(ORACLE) ACCESS(WRITE=YES,SHARE=YES);

   USING ACCCOL;

      NEWITEM ACCCOLCOD LIKE(NAME=ACCCOLCOD) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=NUMBER(9));
      NEWITEM CNTCODETI LIKE(NAME=CNTCODETI) 
             KEY(TYPE=STANDARD);
      NEWITEM CNTORICOD LIKE(NAME=CNTORICOD) 
             KEY(TYPE=STANDARD);
      NEWITEM ESTCOD LIKE(NAME=ESTCOD) 
             KEY(TYPE=STANDARD);
      NEWITEM EXPORDCOD LIKE(NAME=EXPORDCOD) 
             KEY(TYPE=STANDARD);
      NEWITEM MOMINI LIKE(NAME=MOMINI) 
             KEY(TYPE=STANDARD);
             NEWITEM FECINI LIKE(FECINI) 
             REDEFINES(MOMINI[1:8]);
             NEWITEM HORINI LIKE(HORINI) 
             REDEFINES(MOMINI[12:19]);
      NEWITEM RECMAQCOD LIKE(NAME=RECMAQCOD) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=NUMBER(9));
      NEWITEM RECOPECOD LIKE(NAME=RECOPECOD) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=NUMBER(9));
      NEWITEM RFTTERCOD LIKE(NAME=RFTTERCOD) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=NUMBER(9));
      NEWITEM ANCOPE LIKE(NAME=ANCOPE);
      NEWITEM CAJCNT LIKE(NAME=CAJCNT);
      NEWITEM CNTTIP LIKE(NAME=CNTTIP);
      NEWITEM COLEST LIKE(NAME=COLEST) 
             ORACLE(TYPE=CHAR(4));
      NEWITEM COLPESCOD LIKE(NAME=COLPESCOD);
      NEWITEM COLPIC LIKE(NAME=COLPIC) 
             ORACLE(TYPE=CHAR(1));
      NEWITEM COLPRI LIKE(NAME=COLPRI) 
             ORACLE(TYPE=CHAR(4));
      NEWITEM COLSIGCOD LIKE(NAME=COLSIGCOD) 
             ORACLE(TYPE=NUMBER(9));
      NEWITEM COLTIP LIKE(NAME=COLTIP) 
             ORACLE(TYPE=CHAR(4));
      NEWITEM COLTIPPAR LIKE(NAME=COLTIPPAR);
      NEWITEM DENBATCH LIKE(NAME=DENBATCH);
      NEWITEM ENVCOD LIKE(NAME=ENVCOD);
      NEWITEM FECBATCH LIKE(NAME=FECBATCH);
      NEWITEM GESCOLPES LIKE(NAME=GESCOLPES);
      NEWITEM HORBATCH LIKE(NAME=HORBATCH);
      NEWITEM MANIPU LIKE(NAME=MANIPU);
      NEWITEM MOMINICOL LIKE(NAME=MOMINICOL) 
             ORACLE(TYPE=DATE);
      NEWITEM MOVHOR LIKE(NAME=MOVHOR) 
             ORACLE(TYPE=CHAR(4));
      NEWITEM MOVHORPOS LIKE(NAME=MOVHORPOS);
      NEWITEM MOVVER LIKE(NAME=MOVVER);
      NEWITEM NUMBATCH LIKE(NAME=NUMBATCH);
      NEWITEM NUMCNT LIKE(NAME=NUMCNT) 
             ORACLE(TYPE=NUMBER(6));
      NEWITEM ORIASG LIKE(NAME=ORIASG);
      NEWITEM SUBPEDIDO LIKE(NAME=SUBPEDIDO);
      NEWITEM VARIOSCNT LIKE(NAME=VARIOSCNT);
      NEWITEM ZONCARCOD LIKE(NAME=ZONCARCOD);
      NEWITEM ZONCOLCOD LIKE(NAME=ZONCOLCOD);
      NEWITEM ZONPRECOD LIKE(NAME=ZONPRECOD);
   END;

   NEWFILE ACCGEN TYPE(ORACLE) ACCESS(WRITE=YES,SHARE=YES);

   USING ACCGEN;

      NEWITEM GENACCCOD LIKE(NAME=GENACCCOD) 
             KEY(TYPE=STANDARD,PRIMARY,UNIQUE) 
             ORACLE(TYPE=NUMBER(9));
      NEWITEM ARTCOD LIKE(NAME=ARTCOD) 
             KEY(TYPE=STANDARD);
      NEWITEM CNTCOD LIKE(NAME=CNTCOD) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=NUMBER(9));
      NEWITEM ENVCOD LIKE(NAME=ENVCOD) 
             KEY(TYPE=STANDARD);
      NEWITEM ESTADO LIKE(NAME=ESTADO) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=CHAR(4));
      NEWITEM EXPORDCOD LIKE(NAME=EXPORDCOD) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=NUMBER(9));
      NEWITEM EXPORDLIN LIKE(NAME=EXPORDLIN) 
             KEY(TYPE=STANDARD);
      NEWITEM GENACCGRU LIKE(NAME=GENACCGRU) 
             KEY(TYPE=STANDARD);
      NEWITEM NDOCOD LIKE(NAME=NDOCOD) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=NUMBER(9));
      NEWITEM RECCOD LIKE(NAME=RECCOD) 
             KEY(TYPE=STANDARD);
      NEWITEM RECORDCOD LIKE(NAME=RECORDCOD) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=NUMBER(9));
      NEWITEM TARTIP LIKE(NAME=TARTIP) 
             KEY(TYPE=STANDARD) ORACLE(TYPE=CHAR(4));
      NEWITEM BUSTIP LIKE(NAME=BUSTIP);
      NEWITEM CAN LIKE(NAME=CAN) 
             ORACLE(TYPE=NUMBER(5));
      NEWITEM CNTDESCOD LIKE(NAME=CNTDESCOD) 
             ORACLE(TYPE=NUMBER(9));
