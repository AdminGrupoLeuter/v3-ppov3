UNIFYBASE ADAIA_DB;


NEWBASE EDIFILEBASE TYPE(FLAT) 
        LOCATION("/dev/null") ACCESS(WRITE=YES,SHARE=YES) 
        VARIABLE(ITEM-SEPARATOR=" ",RECORD-SEPARATOR="'",ARRAY-SEP="0",
                 DELETE-CHAR="0",QUOTE-STRING=NO);

USING EDIFILEBASE;

   NEWFILE EDIFILE TYPE(FLAT) 
          LOCATION("/dev/null01") ACCESS(WRITE=YES,SHARE=YES);

   USING EDIFILE;

      NEWITEM TEXTLINE LIKE(NAME=TEXTLINE);
   END;
END;




NEWBASE TEXTFILEBASE TYPE(FLAT) 
        LOCATION("/dev/null") ACCESS(WRITE=YES,SHARE=YES) 
        VARIABLE(ITEM-SEPARATOR="<9>",RECORD-SEPARATOR="<10>",ARRAY-SEP="0",
                 DELETE-CHAR="0",QUOTE-STRING=NO);

USING TEXTFILEBASE;

   NEWFILE DEBUG TYPE(FLAT) 
          LOCATION("/dev/null") ACCESS(WRITE=YES,SHARE=YES);

   USING DEBUG;

      NEWITEM TEXTLINE LIKE(NAME=TEXTLINE);
   END;

   NEWFILE MEMOFILE TYPE(FLAT) 
          LOCATION("/dev/null") ACCESS(WRITE=YES,SHARE=YES);

   USING MEMOFILE;

      NEWITEM MEMO LIKE(NAME=MEMO);
   END;

   NEWFILE TEXTFILE TYPE(FLAT) 
          LOCATION("/dev/null") ACCESS(WRITE=YES,SHARE=YES);

   USING TEXTFILE;

      NEWITEM TEXTLINE LIKE(NAME=TEXTLINE);
   END;
END;


