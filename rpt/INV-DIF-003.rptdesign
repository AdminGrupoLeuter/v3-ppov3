            <method name="beforeOpen"><![CDATA[	//                                      
	// Inicializar la string que contendrá el query
	this.queryText = ''
	//
	// <<< ------------------------ >>>
	// <<< TIPO UBICACIONES <> 'CM' >>>
	// <<< ------------------------ >>> 
	//
	// *********************************
	// * Añadir los campos al SELECT 1 *
	// *********************************
	//
	this.queryText = this.queryText + " " + "(SELECT   e.ZONUBIREF," +
	                                  " " +           "e.ZONUBIDES," +
	                                  " " +           "2           AS TIPO_UBIC," +
	                                  " " +           "a.INVTIP," +
	                                  " " +           "b.ESTCOD," +
	                                  " " +           "d.ESTREF," +
	                                  " " +           "c.ALMUBIALI," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN (b.CNTCOD <> 0)" +     
	                                  " " +              "AND (h.MULREF =  'N' OR" + 
	                                  " " +                  "(h.MULREF =  'S' AND i.CNTCOD IS NOT NULL))" + 
	                                  " " +               "THEN (CASE" +
	                                  " " +                       "WHEN h.MULREF = 'S'" +
	                                  " " +                         "THEN f.ARTREF" +
	                                  " " +                       "ELSE   k.ARTREF" +
	                                  " " +                      "END)" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)        AS LEI_ARTREF," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN (b.CNTCOD <> 0)" +
	                                  " " +              "AND (h.MULREF =  'N' OR" +
	                                  " " +                  "(h.MULREF =  'S' AND i.CNTCOD IS NOT NULL))" +
	                                  " " +               "THEN (CASE" +
	                                  " " +                       "WHEN h.MULREF = 'S'" +
	                                  " " +                         "THEN f.ARTDES" +
	                                  " " +                       "ELSE   k.ARTDES" +
	                                  " " +                     "END)" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)        AS LEI_ARTDES," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN (b.CNTCOD <> 0)" +
	                                  " " +              "AND (h.MULREF =  'N' OR" +
	                                  " " +                  "(h.MULREF =  'S' AND i.CNTCOD IS NOT NULL))" +
	                                  " " +               "THEN (CASE" +
	                                  " " +                       "WHEN h.MULREF = 'S'" +
	                                  " " +                         "THEN f.UNIALM" +
	                                  " " +                       "ELSE   k.UNIALM" +
	                                  " " +                     "END)" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)        AS LEI_UNIALM," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN (b.CNTCOD <> 0)" +
	                                  " " +              "AND (h.MULREF =  'N' OR" +
	                                  " " +                  "(h.MULREF =  'S' AND i.CNTCOD IS NOT NULL))" +
	                                  " " +               "THEN (CASE" +
	                                  " " +                       "WHEN h.MULREF = 'S'" +
	                                  " " +                         "THEN f.UNIMIN" +
	                                  " " +                       "ELSE   k.UNIMIN" +
	                                  " " +                     "END)" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)        AS LEI_UNIMIN," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN (b.CNTCOD <> 0)" +
	                                  " " +              "AND (h.MULREF =  'N' OR" +
	                                  " " +                  "(h.MULREF =  'S' AND i.CNTCOD IS NOT NULL))" +
	                                  " " +               "THEN (CASE" +
	                                  " " +                       "WHEN h.MULREF = 'S'" +
	                                  " " +                         "THEN g.NUMUNI" +
	                                  " " +                       "ELSE   l.NUMUNI" +
	                                  " " +                     "END)" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)        AS LEI_NUMUNI_UNIALM," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN (b.CNTCOD <> 0)" +
	                                  " " +              "AND (h.MULREF =  'N' OR" +
	                                  " " +                  "(h.MULREF =  'S' AND i.CNTCOD IS NOT NULL))" +
	                                  " " +               "THEN (CASE" +
	                                  " " +                       "WHEN h.MULREF = 'S'" +
	                                  " " +                         "THEN i.CANUNI" +
	                                  " " +                       "ELSE   j.CANUNI" +
	                                  " " +                     "END)" +
	                                  " " +           "END)        AS LEI_CANUNI," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN (b.CNTCOD <> 0)" +
	                                  " " +              "AND (h.MULREF =  'N' OR" +
	                                  " " +                  "(h.MULREF =  'S' AND i.CNTCOD IS NOT NULL))" +
	                                  " " +               "THEN (CASE" +
	                                  " " +                       "WHEN h.MULREF = 'S'" +
	                                  " " +                         "THEN i.PES" +
	                                  " " +                       "ELSE   j.PES" +
	                                  " " +                     "END)" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)        AS LEI_PES," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN (b.CNTCOD <> 0)" +
	                                  " " +              "AND (h.MULREF =  'N' OR" +
	                                  " " +                  "(h.MULREF =  'S' AND i.CNTCOD IS NOT NULL))" +
	                                  " " +               "THEN 1" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)        AS LEI_PALLETS"
	//
	// ******************************
	// * Añadir el FROM al SELECT 1 *
	// ******************************
	//
	this.queryText = this.queryText + " " +  "FROM     INVNTR a" +
	                                  " " +             "LEFT OUTER JOIN ALMUBI b" +
	                                  " " +                "ON b.ALMUBICOD = a.ALMUBICOD" +
	                                  " " +             "LEFT OUTER JOIN ALMPRV c" +
	                                  " " +                "ON c.ALMUBICOD = a.ALMUBICOD" +
	                                  " " +             "LEFT OUTER JOIN ALMEST d" +
	                                  " " +                "ON d.ESTCOD    = b.ESTCOD" +
	                                  " " +             "LEFT OUTER JOIN ALMZONUBI e" +
	                                  " " +                "ON e.ZONUBICOD = b.ZONUBICOD" +
	                                  " " +             "LEFT OUTER JOIN ART f" +
	                                  " " +                "ON f.ARTCOD    = a.ARTCOD" +
	                                  " " +             "LEFT OUTER JOIN ARTPRE g" +
	                                  " " +                "ON g.ARTCOD    = a.ARTCOD" +
	                                  " " +               "AND g.UNITIP    = f.UNIALM" +
	                                  " " +             "LEFT OUTER JOIN CNTDOR h" +
	                                  " " +                "ON h.CNTCOD    = b.CNTCOD" +
	                                  " " +             "LEFT OUTER JOIN CNTNDO i" +
	                                  " " +                "ON i.CNTEXTCOD = b.CNTCOD" +
	                                  " " +               "AND i.ARTCOD    = a.ARTCOD" +
	                                  " " +             "LEFT OUTER JOIN CNTNDO j" +
	                                  " " +                "ON j.CNTEXTCOD = b.CNTCOD" +
	                                  " " +             "LEFT OUTER JOIN ART k" +
	                                  " " +                "ON k.ARTCOD    = j.ARTCOD" +
	                                  " " +             "LEFT OUTER JOIN ARTPRE l" +
	                                  " " +                "ON l.ARTCOD    = j.ARTCOD" +
	                                  " " +               "AND l.UNITIP    = k.UNIALM"
	//
	// *******************************
	// * Añadir el WHERE al SELECT 1 *
	// *******************************
	//
	// Si parámetro tipo inventario = picking
	if ( params["pstrInvTip"].value == 'P' )
	{
		this.queryText = this.queryText + " " +  "WHERE    a.INVTIP IN ('PICK','INIC')"
	}
	//
	// Si parámetro tipo inventario = altura
	else
	{
		this.queryText = this.queryText + " " +  "WHERE    a.INVTIP IN ('ALTO','INIC')"
	}
	this.queryText = this.queryText + " " +    "AND    a.INVSIT NOT IN  ('PROC','ANUL')" +
	                                  " " +    "AND    a.INVEST =       'DESC'" +
	                                  " " +    "AND    b.UBITIP <>      'CM'" +
	                                  " " +    "AND    c.ALMREF =" + " " + "'" + params["pstrAlmRef"].value + "'"
	//
	// Si está informado el parámetro est. desde ==>
	//   filtrar est. desde-hasta
	if ( params["pstrEstRefDesde"].value != null
	             &amp;&amp;
	     BirtStr.trimRight( params["pstrEstRefDesde"].value ) != '' )
	{
		this.queryText = this.queryText + " " +    "AND    d.ESTREF BETWEEN" +
	                                      " " +           "'" + params["pstrEstRefDesde"].value + "'" +
	                                      " " +                        "AND" +
	                                      " " +           "'" + params["pstrEstRefHasta"].value + "'"
	} 
	//
	this.queryText = this.queryText + ")"
	//
	// ***************************************
	// * Añadir el UNION entre los 2 SELECTs *
	// ***************************************
	//                                
	this.queryText = this.queryText + " " +     "UNION"
	//
	// <<< ----------------------------------------------------------------- >>>
	// <<< TIPO UBICACIONES <> 'CM' - COMPONENTES DE PALLETS MULTIREFERENCIA >>>
	// <<< ----------------------------------------------------------------- >>> 
	//
	// *********************************
	// * Añadir los campos al SELECT 2 *
	// *********************************
	//
	this.queryText = this.queryText + " " + "(SELECT   e.ZONUBIREF," +
	                                  " " +           "e.ZONUBIDES," +
	                                  " " +           "2        AS TIPO_UBIC," +
	                                  " " +           "a.INVTIP," +
	                                  " " +           "b.ESTCOD," +
	                                  " " +           "d.ESTREF," +
	                                  " " +           "c.ALMUBIALI," +
	                                  " " +           "k.ARTREF AS LEI_ARTREF," +
	                                  " " +           "k.ARTDES AS LEI_ARTDES," +
	                                  " " +           "k.UNIALM AS LEI_UNIALM," +
	                                  " " +           "k.UNIMIN AS LEI_UNIMIN," +
	                                  " " +           "l.NUMUNI AS LEI_NUMUNI_UNIALM," +
	                                  " " +           "j.CANUNI AS LEI_CANUNI," +
	                                  " " +           "j.PES    AS LEI_PES," +
	                                  " " +           "1        AS LEI_PALLETS"
	//
	// ******************************
	// * Añadir el FROM al SELECT 2 *
	// ******************************
	//
	this.queryText = this.queryText + " " +  "FROM     INVNTR a" +
	                                  " " +             "LEFT OUTER JOIN ALMUBI b" +
	                                  " " +                "ON b.ALMUBICOD = a.ALMUBICOD" +
	                                  " " +             "LEFT OUTER JOIN ALMPRV c" +
	                                  " " +                "ON c.ALMUBICOD = a.ALMUBICOD" +
	                                  " " +             "LEFT OUTER JOIN ALMEST d" +
	                                  " " +                "ON d.ESTCOD    = b.ESTCOD" +
	                                  " " +             "LEFT OUTER JOIN ALMZONUBI e" +
	                                  " " +                "ON e.ZONUBICOD = b.ZONUBICOD" +
	                                  " " +             "LEFT OUTER JOIN CNTDOR h" +
	                                  " " +                "ON h.CNTCOD    = b.CNTCOD" +
	                                  " " +             "LEFT OUTER JOIN CNTNDO j" +
	                                  " " +                "ON j.CNTEXTCOD = b.CNTCOD" +
	                                  " " +             "LEFT OUTER JOIN ART k" +
	                                  " " +                "ON k.ARTCOD    = j.ARTCOD" +
	                                  " " +             "LEFT OUTER JOIN ARTPRE l" +
	                                  " " +                "ON l.ARTCOD    = j.ARTCOD" +
	                                  " " +               "AND l.UNITIP    = k.UNIALM" 
	//
	// *******************************
	// * Añadir el WHERE al SELECT 2 *
	// *******************************
	//
	// Si parámetro tipo inventario = picking
	if ( params["pstrInvTip"].value == 'P' )
	{
		this.queryText = this.queryText + " " +  "WHERE    a.INVTIP IN ('PICK','INIC')"
	}
	//
	// Si parámetro tipo inventario = altura
	else
	{
		this.queryText = this.queryText + " " +  "WHERE    a.INVTIP IN ('ALTO','INIC')"
	}
	this.queryText = this.queryText + " " +    "AND    a.INVSIT NOT IN  ('PROC','ANUL')" +
	                                  " " +    "AND    a.INVEST =       'DESC'" +
	                                  " " +    "AND    b.UBITIP <>      'CM'" +
	                                  " " +    "AND    c.ALMREF =" + " " + "'" + params["pstrAlmRef"].value + "'"
	//
	// Si está informado el parámetro est. desde ==>
	//   filtrar est. desde-hasta
	if ( params["pstrEstRefDesde"].value != null
	             &amp;&amp;
	     BirtStr.trimRight( params["pstrEstRefDesde"].value ) != '' )
	{
		this.queryText = this.queryText + " " +    "AND    d.ESTREF BETWEEN" +
	                                      " " +           "'" + params["pstrEstRefDesde"].value + "'" +
	                                      " " +                        "AND" +
	                                      " " +           "'" + params["pstrEstRefHasta"].value + "'"
	} 
	//
	this.queryText = this.queryText + " " +    "AND    h.MULREF = 'S'" +
	                                  " " +    "AND    j.ARTCOD NOT IN  (SELECT q.ARTCOD" +
	                                  " " +                             "FROM   INVNTR q" +
	                                  " " +                             "WHERE  q.INVTIP    <> 'INVR'" +
	                                  " " +                               "AND  q.ALMUBICOD =  a.ALMUBICOD" +
	                                  " " +                               "AND  q.ARTCOD    =  j.ARTCOD))" 
	//
	// ***************************************
	// * Añadir el UNION entre los 2 SELECTs *
	// ***************************************
	//                                
	this.queryText = this.queryText + " " +     "UNION"
	//
	// <<< ----------------------- >>>
	// <<< TIPO UBICACIONES = 'CM' >>>
	// <<< ----------------------- >>> 
	//
	// *********************************
	// * Añadir los campos al SELECT 3 *
	// *********************************
	//
	this.queryText = this.queryText + " " + "(SELECT   e.ZONUBIREF," +
	                                  " " +           "e.ZONUBIDES," +
	                                  " " +           "1                          AS TIPO_UBIC," +
	                                  " " +           "a.INVTIP," +
	                                  " " +           "b.ESTCOD," +
	                                  " " +           "d.ESTREF," +
	                                  " " +           "NULL                       AS ALMUBIALI," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN k.ARTREF" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)                       AS LEI_ARTREF," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN k.ARTDES" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)                       AS LEI_ARTDES," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN k.UNIALM" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)                       AS LEI_UNIALM," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN k.UNIMIN" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)                       AS LEI_UNIMIN," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN l.NUMUNI" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)                       AS LEI_NUMUNI_UNIALM," +
	                                  " " +           "SUM(CASE" +
	                                  " " +                 "WHEN b.CNTCOD <> 0" +
	                                  " " +                   "THEN j.CANUNI" +
	                                  " " +                 "ELSE   NULL" +
	                                  " " +               "END)                   AS LEI_CANUNI," +
	                                  " " +          "SUM(CASE" +
	                                  " " +                "WHEN b.CNTCOD <> 0" +
	                                  " " +                  "THEN j.PES" +
	                                  " " +                "ELSE   NULL" +
	                                  " " +              "END)                    AS LEI_PES," +
	                                  " " +           "COUNT(CASE" +
	                                  " " +                   "WHEN b.CNTCOD <> 0" +
	                                  " " +                     "THEN 1" +
	                                  " " +                 "END)                 AS LEI_PALLETS"
	//
	// ******************************
	// * Añadir el FROM al SELECT 3 *
	// ******************************
	//
	this.queryText = this.queryText + " " +  "FROM     INVNTR a" +
	                                  " " +             "LEFT OUTER JOIN ALMUBI b" +
	                                  " " +                "ON b.ALMUBICOD = a.ALMUBICOD" +
	                                  " " +             "LEFT OUTER JOIN ALMPRV c" +
	                                  " " +                "ON c.ALMUBICOD = a.ALMUBICOD" +
	                                  " " +             "LEFT OUTER JOIN ALMEST d" +
	                                  " " +                "ON d.ESTCOD    = b.ESTCOD" +
	                                  " " +             "LEFT OUTER JOIN ALMZONUBI e" +
	                                  " " +                "ON e.ZONUBICOD = b.ZONUBICOD" +
	                                  " " +             "LEFT OUTER JOIN CNTNDO j" +
	                                  " " +                "ON j.CNTEXTCOD = b.CNTCOD" +
	                                  " " +             "LEFT OUTER JOIN ART k" +
	                                  " " +                "ON k.ARTCOD    = j.ARTCOD" +
	                                  " " +             "LEFT OUTER JOIN ARTPRE l" +
	                                  " " +                "ON l.ARTCOD    = j.ARTCOD" +
	                                  " " +               "AND l.UNITIP    = k.UNIALM" 
	//
	// *******************************
	// * Añadir el WHERE al SELECT 3 *
	// *******************************
	//
	// Si parámetro tipo inventario = picking
	if ( params["pstrInvTip"].value == 'P' )
	{
		this.queryText = this.queryText + " " +  "WHERE    a.INVTIP IN ('PICK','INIC')"
	}
	//
	// Si parámetro tipo inventario = altura
	else
	{
		this.queryText = this.queryText + " " +  "WHERE    a.INVTIP IN ('ALTO','INIC')"
	}
	this.queryText = this.queryText + " " +    "AND    a.INVSIT NOT IN  ('PROC','ANUL')" +
	                                  " " +    "AND    a.INVEST =       'DESC'" +
	                                  " " +    "AND    b.UBITIP =       'CM'" +
	                                  " " +    "AND    c.ALMREF =" + " " + "'" + params["pstrAlmRef"].value + "'"
	//
	// Si está informado el parámetro est. desde ==>
	//   filtrar est. desde-hasta
	if ( params["pstrEstRefDesde"].value != null
	             &amp;&amp;
	     BirtStr.trimRight( params["pstrEstRefDesde"].value ) != '' )
	{
		this.queryText = this.queryText + " " +    "AND    d.ESTREF BETWEEN" +
	                                      " " +           "'" + params["pstrEstRefDesde"].value + "'" +
	                                      " " +                        "AND" +
	                                      " " +           "'" + params["pstrEstRefHasta"].value + "'"
	} 
	//
	// **********************************
	// * Añadir el GROUP BY al SELECT 3 *
	// **********************************
	//                                
	this.queryText = this.queryText + " " +  "GROUP BY e.ZONUBIREF," +
	                                  " " +           "e.ZONUBIDES," +
	                                  " " +           "a.INVTIP," +
	                                  " " +           "b.ESTCOD," +
	                                  " " +           "d.ESTREF," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN k.ARTREF" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN k.ARTDES" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN k.UNIALM" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN k.UNIMIN" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END)," +
	                                  " " +          "(CASE" +
	                                  " " +             "WHEN b.CNTCOD <> 0" +
	                                  " " +               "THEN l.NUMUNI" +
	                                  " " +             "ELSE   NULL" +
	                                  " " +           "END))"
	//
	// ********************************
	// * Añadir el ORDER BY al SELECT *
	// ********************************
	//   
	this.queryText = this.queryText + " " + "ORDER BY 1," +
	                                  " " +          "3," +
	                                  " " +          "4," +
	                                  " " +          "5"]]></method>
