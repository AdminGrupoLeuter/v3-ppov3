            <property name="queryText">SELECT   b.EXPORDREF,
         a.EXPORDLIN,
         c.ARTREF,
         c.ARTDES,
         c.UNIEXP,
         d.NUMUNI AS NUMUNI_UNIEXP, 
         c.UNIMIN,
         a.CAN          
FROM     ACCTRA a
           LEFT OUTER JOIN EXPORDCAB b
              ON b.EXPORDCOD = a.EXPORDCOD
           LEFT OUTER JOIN ART c
              ON c.ARTCOD    = a.ARTCOD
           LEFT OUTER JOIN ARTPRE d
              ON d.ARTCOD    = a.ARTCOD
             AND d.UNITIP    = c.UNIEXP          
WHERE    a.ACCCOD = ?</property>
