            <property name="queryText">SELECT   a.CNTDORREF,
         d.TERREF,
         d.TERDES,
         b.LOTE,
         b.CRIROT,
        (CASE
           WHEN f.NUMUNI IS NOT NULL
            AND f.NUMUNI &lt;>     0
             THEN TRUNC((a.CANUNI / f.NUMUNI),0)
           ELSE   0
         END)     AS CANUNI_UNIALM,
         e.UNIALM,
        (CASE
           WHEN f.NUMUNI IS NOT NULL
            AND f.NUMUNI &lt;>     0
             THEN MOD(a.CANUNI, f.NUMUNI)
           ELSE   a.CANUNI
         END)     AS CANUNI_UNIMIN,
         e.UNIMIN,
         a.MOMMOV,
         b.RECCOD 
FROM     HISMOVALM a 
           LEFT OUTER JOIN RECLINNDO b
              ON b.CNTEXTCOD = a.CNTCOD
           LEFT OUTER JOIN RECCAB c
              ON c.RECCOD    = b.RECCOD
           LEFT OUTER JOIN TER d
              ON d.TERCOD    = c.TERCOD
           LEFT OUTER JOIN ART e
              ON e.ARTCOD    = a.ARTCOD
           LEFT OUTER JOIN ARTPRE f
              ON f.ARTCOD    = a.ARTCOD
             AND f.UNITIP    = e.UNIALM</property>
