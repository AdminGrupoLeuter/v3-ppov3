            <method name="beforeOpen"><![CDATA[//
// *****************************
// * Añadir el WHERE al SELECT *
// *****************************
//
this.queryText = this.queryText + " " + "WHERE    a.ARTCOD =" + " " + params["pdecArtCod"].value +
                                  " " +   "AND    a.MOMMOV = (SELECT MIN(p.MOMMOV)" +
                                  " " +                      "FROM   HISMOVALM p" + 
                                  " " +                      "WHERE  p.CNTCOD = a.CNTCOD)"
//
// Si está informado el parámetro lote ==>
//   filtrar dicho campo
if ( params["pstrCriRot"].value != null 
            &amp;&amp; 
     BirtStr.trimRight( params["pstrCriRot"].value ) != '' )
{
	this.queryText = this.queryText + " " + "AND    a.CRIROT =" + " " + "'" + params["pstrCriRot"].value + "'"
}  
//
// Si está informado el parámetro lote ==>
//   filtrar dicho campo
if ( params["pstrLote"].value != null 
            &amp;&amp; 
     BirtStr.trimRight( params["pstrLote"].value ) != '' )
{
	this.queryText = this.queryText + " " + "AND    a.LOTE =" + " " + "'" + params["pstrLote"].value + "'"
}
//
// ********************************
// * Añadir el ORDER BY al SELECT *
// ********************************
//
this.queryText = this.queryText + " " + "ORDER BY a.CNTDORREF"]]></method>
