            <method name="beforeOpen"><![CDATA[//
// *****************************
// * Añadir el WHERE al SELECT *
// *****************************
//
this.queryText = this.queryText + " " + "WHERE    a.EXPORDCOD =" + " " + params["pdecExpOrdCod"].value +
                                  " " +   "AND    a.ARTPADCOD IN (" + params["strLisArtPadCod"].value + ")"]]></method>
            <list-property name="resultSet">
                <structure>
                    <property name="position">1</property>
                    <property name="name">ARTREF</property>
                    <property name="nativeName">ARTREF</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
                <structure>
                    <property name="position">2</property>
                    <property name="name">ARTDES</property>
                    <property name="nativeName">ARTDES</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
                <structure>
                    <property name="position">3</property>
                    <property name="name">UNIEXP</property>
                    <property name="nativeName">UNIEXP</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
                <structure>
                    <property name="position">4</property>
                    <property name="name">UNIMIN</property>
                    <property name="nativeName">UNIMIN</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
                <structure>
                    <property name="position">5</property>
                    <property name="name">NUMUNI_UNIEXP</property>
                    <property name="nativeName">NUMUNI_UNIEXP</property>
                    <property name="dataType">float</property>
                    <property name="nativeDataType">8</property>
                </structure>
                <structure>
                    <property name="position">6</property>
                    <property name="name">CANUNI_DEV</property>
                    <property name="nativeName">CANUNI_DEV</property>
                    <property name="dataType">float</property>
                    <property name="nativeDataType">8</property>
                </structure>
                <structure>
                    <property name="position">7</property>
                    <property name="name">CNTDORREF</property>
                    <property name="nativeName">CNTDORREF</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
                <structure>
                    <property name="position">8</property>
                    <property name="name">MUELLE</property>
                    <property name="nativeName">MUELLE</property>
                    <property name="dataType">string</property>
                    <property name="nativeDataType">12</property>
                </structure>
            </list-property>
            <property name="queryText">SELECT   b.ARTREF,
         b.ARTDES,
         b.UNIEXP,
         b.UNIMIN,
         c.NUMUNI                 AS NUMUNI_UNIEXP,  
        (a.CANSER - 
          (COALESCE((SELECT (CASE
                               WHEN q.CANREL &lt;> 0 
                                 THEN ROUND((q.CANSER / q.CANREL),0,1)
                              END) 
                      FROM   EXPORDLIN p
                               INNER JOIN EXPLIN q
                                  ON q.EXPORDCOD = p.EXPORDCOD
                                 AND q.EXPORDLIN = p.EXPORDLIN
                      WHERE  p.EXPORDCOD = a.EXPORDCOD
                        AND  p.EXPORDLIN = (SELECT MIN(r.EXPORDLIN)
                                            FROM   EXPORDLIN r
                                            WHERE  r.EXPORDCOD = a.EXPORDCOD
                                              AND  r.ARTPADCOD = a.ARTPADCOD)),0) 
               *
           a.CANREL ))            AS CANUNI_DEV,
        (CASE 
           WHEN d.CNTCOD &lt;> d.CNTEXTCOD
             THEN e.CNTDORREF
           ELSE   f.CNTDORREF
         END)                     AS CNTDORREF,
        (CASE
           WHEN h.ESTTIP = 'MO'
             THEN j.ESTREF
           ELSE   h.ESTREF
         END)                     AS MUELLE
FROM     EXPLIN a
           LEFT OUTER JOIN ART b
              ON b.ARTCOD       = a.ARTCOD
           LEFT OUTER JOIN ARTPRE c
              ON c.ARTCOD       = a.ARTCOD
             AND c.UNITIP       = b.UNIEXP
           LEFT OUTER JOIN CNTNDO d
              ON d.EXPORDCODORI = a.EXPORDCOD
             AND d.EXPORDLINORI = a.EXPORDLIN 
           LEFT OUTER JOIN CNTDOR e
              ON e.CNTCOD       = d.CNTEXTCOD
           LEFT OUTER JOIN CNTDOR f 
              ON f.CNTCOD       = d.CNTCOD
           LEFT OUTER JOIN ALMUBI g
              ON g.CNTCOD       = d.CNTEXTCOD
           LEFT OUTER JOIN ALMEST h
              ON h.ESTCOD       = g.ESTCOD   
           LEFT OUTER JOIN EXPORDCAB i
              ON i.EXPORDCOD    = a.EXPORDCOD
           LEFT OUTER JOIN ALMEST j
              ON j.ESTCOD       = i.MUEEXP</property>
        </oda-data-set>
