            <property name="queryText">SELECT   d.ALMUBIALI,
         e.ARTREF,
         e.ARTDES,
         b.CAN,   
         e.UNIEXP,
         e.UNIMIN,  
         g.NUMUNI AS NUMUNI_UNIEXP,
         b.ARTCOD,
         f.UBIPIC,
         f.UBIPICDIN         
FROM     ACCCOL a
           INNER JOIN ACCTRA b
              ON b.ACCCOLCOD = a.ACCCOLCOD
           LEFT OUTER JOIN EXPORDCAB c
              ON c.EXPORDCOD = b.EXPORDCOD
           LEFT OUTER JOIN ALMPRV d
              ON d.ALMUBICOD = b.UBIORICOD
           LEFT OUTER JOIN ART e
              ON e.ARTCOD    = b.ARTCOD
           LEFT OUTER JOIN ARTALM f
              ON f.ARTCOD    = b.ARTCOD
             AND f.ALMCOD    = c.ALMCOD 
           LEFT OUTER JOIN ARTPRE g
              ON g.ARTCOD    = b.ARTCOD
             AND g.UNITIP    = e.UNIEXP      
WHERE    a.EXPORDCOD = ?
  AND    b.ACCTIPTAR = 'PCNS'  
ORDER BY b.ACCORD,
         b.ACCORDREC</property>
