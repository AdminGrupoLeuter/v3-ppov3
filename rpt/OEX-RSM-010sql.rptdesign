                                <structure>
                                    <property name="name">decUdsMin</property>
                                    <expression name="expression">if ( dataSetRow["NUMUNI"] != null
                       &amp;&amp;
     dataSetRow["NUMUNI"] != 0 )
{
        var x = ( dataSetRow["CAN"] )
        if ( x &lt; 0 )
        {
                var y = -1
        }
        else
        {
                var y = 1
        }
        BirtMath.mod( ( x * y ), dataSetRow["NUMUNI"] ) * y
}
else
{
        dataSetRow["CAN"]
}</expression>
