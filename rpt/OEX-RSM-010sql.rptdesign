            <method name="beforeOpen"><![CDATA[//
// Inicializar string de la consulta
this.queryText = ''
//
// *********************************
// * Añadir los campos al SELECT 1 *
// *********************************
//
this.queryText = this.queryText + " " + "(SELECT   a.EXPORDSIT," +
                                  " " +           "a.EXPORDTIP," +
                                  " " +           "a.EXPORDSUBTIP," + 
                                  " " +           "b.TERREF," +
                                  " " +           "b.TERDES," +   
                                  " " +           "a.EXPORDREF," + 
                                  " " +           "a.EXPORDCOD," + 
                                  " " +           "a.MOMEXP," +                                  
                                  " " +           "COUNT(DISTINCT(d.CNTEXTCOD)) AS CNT_CONT," + 
                                  " " +           "ROUND(SUM(CASE" +
                                  " " +                       "WHEN f.NUMUNI IS NOT NULL" +
                                  " " +                        "AND f.NUMUNI <>     0" +
                                  " " +                         "THEN d.CANUNI / f.NUMUNI" +
                                  " " +                       "ELSE   0" +
                                  " " +                     "END),0,1)          AS SUM_UDSEXP," +                                  
                                  " " +           "COALESCE(SUM(d.CANUNI),0)    AS SUM_TOTUDSMIN"
//
// ******************************
// * Añadir el FROM al SELECT 1 *
// ******************************
//
this.queryText = this.queryText + " " +  "FROM     EXPORDCAB a" +
                                  " " +             "LEFT OUTER JOIN TER b" +
                                  " " +                "ON b.TERCOD    = a.TERCOD" +
                                  " " +             "INNER JOIN EXPORDLIN c" +
                                  " " +                "ON c.EXPORDCOD = a.EXPORDCOD" +
                                  " " +             "INNER JOIN CNTNDO d" +
                                  " " +                "ON d.EXPORDCOD = a.EXPORDCOD" +
                                  " " +               "AND d.EXPORDLIN = c.EXPORDLIN" +
                                  " " +             "LEFT OUTER JOIN ART e" +
                                  " " +                "ON e.ARTCOD    = c.ARTCOD" +
                                  " " +             "LEFT OUTER JOIN ARTPRE f" +
                                  " " +                "ON f.ARTCOD    = c.ARTCOD" +
                                  " " +               "AND f.UNITIP    = e.UNIEXP"
//
// *******************************
// * Añadir el WHERE al SELECT 1 *
// *******************************
//
this.queryText = this.queryText + " " +  "WHERE   a.FECORD BETWEEN" +
                                  " " +          "dbo.LTR_TO_DATE('" + params["pstrFecOrdDesde"].value +
                                  " " +                           "00:00:00'," +
                                  " " +                          "'DD/MM/YYYY HH24:MI:SS')" +
                                  " " +                       "AND" +
                                  " " +          "dbo.LTR_TO_DATE('" + params["pstrFecOrdHasta"].value +
                                  " " +                           "00:00:00'," +
                                  " " +                          "'DD/MM/YYYY HH24:MI:SS')" +
                                  " " +    "AND   d.CANUNI > 0"
//
// Si está informado el parámetro sit. orden ==>
//   filtrar dicho campo en tabla EXPORDCAB
if ( params["pstrExpOrdSit"].value != null
            &amp;&amp;
     BirtStr.trimRight( params["pstrExpOrdSit"].value ) != '' )
{
	this.queryText = this.queryText + " " + "AND    a.EXPORDSIT =" + " " + "'" + params["pstrExpOrdSit"].value + "'"
}
//
// **********************************
// * Añadir el GROUP BY al SELECT 1 *
// **********************************
//
this.queryText = this.queryText + " " + "GROUP BY a.EXPORDSIT," +
                                  " " +          "a.EXPORDTIP," +
                                  " " +          "a.EXPORDSUBTIP," +
                                  " " +          "a.EXPORDREF," +
                                  " " +          "a.EXPORDCOD," +
                                  " " +          "a.MOMEXP," +                                 
                                  " " +          "b.TERREF," +
                                  " " +          "b.TERDES)"                                 
//
// **********************************
// * Añadir el UNION de los SELECTs *
// **********************************
//
this.queryText = this.queryText + " " +   "UNION"
//
// *********************************
// * Añadir los campos al SELECT 2 *
// *********************************
//
this.queryText = this.queryText + " " + "(SELECT   a.EXPORDSIT," +
                                  " " +           "a.EXPORDTIP," +
                                  " " +           "a.EXPORDSUBTIP," + 
                                  " " +           "b.TERREF," +
                                  " " +           "b.TERDES," +
                                  " " +           "a.EXPORDREF," + 
                                  " " +           "a.EXPORDCOD," +
                                  " " +           "a.MOMEXP," +                                  
                                  " " +           "0                            AS CNT_CONT," +
                                  " " +           "ROUND(SUM(CASE" +
                                  " " +                       "WHEN f.NUMUNI IS NOT NULL" +
                                  " " +                        "AND f.NUMUNI <>     0" +
                                  " " +                         "THEN c.CANPED / f.NUMUNI" +
                                  " " +                       "ELSE   0" +
                                  " " +                     "END),0,1)          AS SUM_UDSEXP," +  
                                  " " +           "COALESCE(SUM(c.CANPED),0)    AS SUM_TOTUDSMIN"
//
// ******************************
// * Añadir el FROM al SELECT 2 *
// ******************************
//
this.queryText = this.queryText + " " +  "FROM     EXPORDCAB a" +
                                  " " +             "LEFT OUTER JOIN TER b" +
                                  " " +                "ON b.TERCOD    = a.TERCOD" +
                                  " " +             "INNER JOIN EXPORDLIN c" +
                                  " " +                "ON c.EXPORDCOD = a.EXPORDCOD" +
                                  " " +             "LEFT OUTER JOIN ART e" +
                                  " " +                "ON e.ARTCOD    = c.ARTCOD" +
                                  " " +             "LEFT OUTER JOIN ARTPRE f" +
                                  " " +                "ON f.ARTCOD    = c.ARTCOD" +
                                  " " +               "AND f.UNITIP    = e.UNIEXP"
//
// *******************************
// * Añadir el WHERE al SELECT 2 *
// *******************************
//
this.queryText = this.queryText + " " +  "WHERE   a.FECORD BETWEEN" +
                                  " " +          "dbo.LTR_TO_DATE('" + params["pstrFecOrdDesde"].value +
                                  " " +                           "00:00:00'," +
                                  " " +                          "'DD/MM/YYYY HH24:MI:SS')" +
                                  " " +                       "AND" +
                                  " " +          "dbo.LTR_TO_DATE('" + params["pstrFecOrdHasta"].value +
                                  " " +                           "00:00:00'," +
                                  " " +                          "'DD/MM/YYYY HH24:MI:SS')"
//
// Si está informado el parámetro sit. orden ==>
//   filtrar dicho campo en tabla EXPORDCAB
if ( params["pstrExpOrdSit"].value != null
            &amp;&amp;
     BirtStr.trimRight( params["pstrExpOrdSit"].value ) != '' )
{
	this.queryText = this.queryText + " " + "AND    a.EXPORDSIT =" + " " + "'" + params["pstrExpOrdSit"].value + "'"
}
//
// **********************************
// * Añadir el GROUP BY al SELECT 2 *
// **********************************
//
this.queryText = this.queryText + " " + "GROUP BY a.EXPORDSIT," +
                                  " " +          "a.EXPORDTIP," +
                                  " " +          "a.EXPORDSUBTIP," +
                                  " " +          "a.EXPORDREF," +
                                  " " +          "a.EXPORDCOD," +
                                  " " +          "a.MOMEXP," +                                
                                  " " +          "b.TERREF," +
                                  " " +          "b.TERDES)"                                 
//
// **********************************
// * Añadir el ORDER BY al SELECT 2 *
// **********************************
//
this.queryText = this.queryText + " " + "ORDER BY 1," +
                                  " " +          "2," +
                                  " " +          "3," +
                                  " " +          "4"]]></method>
