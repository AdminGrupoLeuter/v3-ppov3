 23  		      ((ESTORICOD IN (iEstcod,iEstproPas) or ESTDESCOD IN (iEstCod,iEstPropas)) AND iEstProPas<>0)
 24  		      OR
 25  		      ((ESTORICOD IN (iEstcod) or ESTDESCOD IN (iEstCod)) AND iEstProPas=0)
 26  		      );
 27  	     end if;
 28  	end if;
 29  	RETURN tmpVar;
 30  	EXCEPTION
 31  	  WHEN NO_DATA_FOUND THEN
 32  	    RETURN 0;
