PARM L_PICINV FORMAT(TYPE=INT,LENGTH=1,SUBTYPE=NUMERIC);
SET TITLE("Trabajo de Picking: Realizar una cola");
SET FILES(FINAL-CLOSE=ALL);
DCL I FORMAT(TYPE=INT,LENGTH=1,SUBTYPE=NUMERIC);
